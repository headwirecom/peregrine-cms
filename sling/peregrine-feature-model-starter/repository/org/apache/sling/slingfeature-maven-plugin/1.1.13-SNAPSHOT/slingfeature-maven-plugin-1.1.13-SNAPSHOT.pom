            <scope>provided</scope>
        </dependency>
        <!-- for converting Maven to OSGi versions -->
        <dependency>
            <groupId>biz.aQute.bnd</groupId>
            <artifactId>biz.aQute.bndlib</artifactId>
            <version>4.2.0</version>
        </dependency>
        <dependency>
            <groupId>org.codehaus.plexus</groupId>
