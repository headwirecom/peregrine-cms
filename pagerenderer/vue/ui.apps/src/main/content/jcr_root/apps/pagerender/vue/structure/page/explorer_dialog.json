{ "fields":[
  {
    "type": "input",
    "inputType": "text",
    "model": "name",
    "label": "Name",
    "readonly": true,
    "placeholder": "page name"
  },
  {
    "type": "input",
    "inputType": "text",
    "model": "title",
    "x-model": "jcr:title",
    "label": "Title",
    "placeholder": "page title"
  },
  {
    "type": "material-datetime",
    "inputType": "text",
    "model": "created",
    "label": "Created",
    "readonly": true,
    "placeholder": "created"
  },
  {
    "type": "input",
    "inputType": "text",
    "model": "createdBy",
    "label": "Created By",
    "readonly": true,
    "placeholder": "created by"
  },
  {
    "type": "material-datetime",
    "inputType": "text",
    "model": "lastModified",
    "label": "Last Modified",
    "readonly": true,
    "placeholder": "lastModified"
  },
  {
    "type": "input",
    "inputType": "text",
    "model": "lastModifiedBy",
    "label": "Last Modified By",
    "readonly": true,
    "placeholder": "lastModifiedBy"
  },
  {
    "type": "material-multiselect",
    "model": "tags",
    "label": "Tags",
    "placeholder": "tags",
    "selectOptions": {
      "multiple": true,
      "trackBy": "name",
      "label": "name",
      "searchable": true,
      "clearOnSelect": true,
      "closeOnSelect": false,
      "taggable": true
    },
    "valuesFrom": "/content/objects/tags.infinity.json"
  },
  {
    "type": "material-textarea",
    "inputType": "text",
    "model": "description",
    "label": "Description",
    "rows": 10,
    "placeholder": "enter a description for this page"
  },
  {
    "type": "material-select",
    "label": "Protocol",
    "model": "protocol",
    "selectOptions": {
      "name": "name",
      "value": "value"
    },
    "values": [
      { "name": "https://", "value": "https://" },
      { "name": "http://", "value": "http://" }
    ],
    "hint": "The name of the scheme used to make the request."
  },
  {
    "type": "material-select",
    "label": "Hostname",
    "model": "hostname",
    "selectOptions": {
      "name": "name",
      "value": "value"
    },
    "values": [
      { "name": "www.metaproc.com", "value": "www.metaproc.com" }
    ],
    "hint": "The host name of the server to which the request is sent."
  },
  {
    "type": "input",
    "inputType": "text",
    "label": "Canonical Link",
    "model": "canonicalLink",
    "x-model": "canonicalLink",
    "placeholder": "canonical link element"
  },
  {
    "type": "material-checkbox",
    "label": "Exclude from Navigation",
    "model": "hideInNav",
    "hint": "This is to indicate whether the page should be exclude from navigation."
  }
]}
