{
	"tags": {
		"allowUnkownTags": true
	},
	"opts": {
		"recurse": true,
		"destination": "target/site/jsdoc"
	},
	"plugins": ["node_modules/jsdoc-vue", "plugins/markdown"],
	"source": {
		"include": [
			"admin-base/ui.apps/src/main/js",
			"admin-base/ui.apps/src/main/content/jcr_root/apps",
			"./admin-base/ui.apps/readme.md",
			"./admin-base/ui.apps/package.json",
			"pagerenderer/vue/ui.apps/src/main/js",
			"pagerenderer/vue/ui.apps/src/main/content/jcr_root/apps",
			"./pagerenderer/vue/ui.apps/readme.md",
			"./pagerenderer/vue/ui.apps/package.json"
		],
		"includePattern": "\\.(vue|js|md)$"
	}
}
